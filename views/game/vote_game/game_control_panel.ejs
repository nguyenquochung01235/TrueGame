<!DOCTYPE html>
<html lang="en">
<%- include('header'); -%>
    <body class="hold-transition sidebar-mini layout-fixed">
      <% if (isGameExist) { %>
         <%- include('popup_update_game_infor') %>
         <%- include('popup_add_new_candidate') %>
         <%- include('popup_update_candidate') %>
         <%- include('popup_add_new_examiner') %>
         <%- include('popup_update_examiner') %>
      <% } %>
      
        <div class="wrapper">
            <!-- Preloader -->
            <div class="preloader flex-column justify-content-center align-items-center">
                <img class="animation__shake" src="/template/image/vote.png" alt="VoteGameLogo" height="100" width="100">
                <span class="brand-text font-weight-bold"> <%=title%> </span>
            </div>



            <!-- Main Sidebar Container -->
            <aside class="main-sidebar sidebar-dark-primary elevation-4">
                <!-- Brand Logo -->
                <a href="#" class="brand-link">
                    <span class="brand-text font-weight-bold">
                        <%=title%>
                    </span>
                </a>

                <!-- Sidebar -->
                <div class="sidebar">
                    <!-- Sidebar user panel (optional) -->
                    <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                        <div class="image">
                            <img src="/template/dist/img/user2-160x160.jpg" class="img-circle elevation-2"
                                alt="User Image">
                        </div>
                        <div class="info">
                            <a href="#" class="d-block">Game Master</a>
                        </div>
                    </div>

                    <!-- SidebarSearch Form -->

                    <!-- Sidebar Menu -->
                    <nav class="mt-2">
                        <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu"
                            data-accordion="false">
                            <!-- Add icons to the links using the .nav-icon class
               with font-awesome or any other icon font library -->
                            <li class="nav-item menu-open">
                                <a href="#" class="nav-link active">
                                    <i class="nav-icon fas fa-tachometer-alt"></i>
                                    <p>
                                        Game Setting
                                        <i class="right fas fa-angle-left"></i>
                                    </p>
                                </a>
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a href="/vote-game/setting" class="nav-link active">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Game Management</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/vote-game/setting/login" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Log Out</p>
                                        </a>
                                    </li>
                                    <!-- <li class="nav-item">
                                        <a href="./index3.html" class="nav-link">
                                            <i class="far fa-circle nav-icon"></i>
                                            <p>Candidate Management</p>
                                        </a>
                                    </li> -->
                                </ul>
                            </li>

                        </ul>
                    </nav>
                    <!-- /.sidebar-menu -->
                </div>
                <!-- /.sidebar -->
            </aside>

            <!-- Content Wrapper. Contains page content -->
            <div class="content-wrapper">
                <!-- Content Header (Page header) -->
                <div class="content-header bg-white mb-2">
                    <div class="container-fluid">
                        <div class="row mb-2" style="align-items: center;">
                                <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                                <h3 class="m-0 font-weight-bold">GAME CONTROL PANEL</h1>
                        </div><!-- /.row -->
                    </div><!-- /.container-fluid -->
                </div>
                <!-- /.content-header -->

                <!-- Main content -->
                <section class="content">
                    <div class="container-fluid">
                        <% if (isGameExist) { %>
                          <!-- Info boxes -->
                          <div class="row" id="game_info">
                            <div class="col-12 col-sm-6 col-md-3">
                                <div class="info-box">
                                    <span class="info-box-icon bg-info elevation-1"><i
                                            class="fas fa-chalkboard-teacher"></i></span>

                                    <div class="info-box-content">
                                        <span class="info-box-text">Giám Khảo</span>
                                        <span class="info-box-number">
                                          <%- gameData.examiner.total %>
                                        </span>
                                    </div>
                                    <!-- /.info-box-content -->
                                </div>
                                <!-- /.info-box -->
                            </div>
                            <div class="col-12 col-sm-6 col-md-3">
                                <div class="info-box mb-3">
                                    <span class="info-box-icon bg-warning elevation-1"><i
                                            class="fas fa-users"></i></span>

                                    <div class="info-box-content">
                                        <span class="info-box-text">Thí Sinh</span>
                                        <span class="info-box-number">
                                          <%-  gameData?.candidate?.total || 0 %>
                                        </span>
                                    </div>
                                    <!-- /.info-box-content -->
                                </div>
                                <!-- /.info-box -->
                            </div>
                            <!-- /.col -->
                            <div class="col-12 col-sm-6 col-md-3">
                                <div class="info-box mb-3">
                                    <span class="info-box-icon bg-primary elevation-1"><i
                                            class="fas fa-thumbs-up"></i></span>

                                    <div class="info-box-content">
                                        <span class="info-box-text">Likes</span>
                                        <span class="info-box-number" id="number_ratting">0</span>
                                    </div>
                                    <!-- /.info-box-content -->
                                </div>
                                <!-- /.info-box -->
                            </div>
                            <!-- /.col -->

                            <!-- fix for small devices only -->
                            <div class="clearfix hidden-md-up"></div>

                            <div class="col-12 col-sm-6 col-md-3">
                                <div class="info-box mb-3">
                                    <span class="info-box-icon bg-success elevation-1"><i class="fas fa-eye"></i></span>

                                    <div class="info-box-content">
                                        <span class="info-box-text">Viewer</span>
                                        <span class="info-box-number" id="number_viewer">0</span>
                                    </div>
                                    <!-- /.info-box-content -->
                                </div>
                                <!-- /.info-box -->
                            </div>
                            <!-- /.col -->
                            <!-- /.col -->
                          </div>
                          <!-- End Info boxes -->
                          
                          <!-- Game Box -->
                          <div class="game_zone">
                            <div class="row mb-2">
                                <!-- left column -->
                                <div class="col-md-6">
                                    <!-- general form elements -->
                                    <div class="card card-success">
                                        <div class="card-header">
                                            <h3 class="card-title font-weight-bold">
                                              <%- gameData.game.name_game %>
                                            </h3>
                                        </div>
                                        <!-- /.card-header -->
                                        <!-- form start -->
                                        <div id="game_box">
                                            <div class="card-body row" id="game_box_body" style="height: 500px;">
                                                <div class="col-md-6">
                                                    <div class="card card-success">
                                                        <div class="card-header">
                                                            <h3 class="card-title font-weight-bold">
                                                              Link Web View
                                                            </h3>
                                                            <div class="card-tools">
                                                                <button type="button" class="btn btn-success btn-sm"
                                                                    data-card-widget="collapse" title="Collapse">
                                                                    <i class="fas fa-minus"></i>
                                                                </button>
                                                            </div>
                                                        </div>
                                                        <div class="card-body">
                                                            <button class="btn btn-success col mt-2 p-2">
                                                                <a href="/vote-game" target="_blank" style="color: white;">
                                                                    Vote Home Page
                                                                </a>
                                                                
                                                            </button> 
                                                            <button class="btn btn-success col mt-2 p-2">
                                                                <a href="/vote-game/examiner" target="_blank" style="color: white;">
                                                                    Vote Examiner Page
                                                                </a>
                                                            </button>
                                                            <button class="btn btn-success col mt-2 p-2">
                                                                <a href="/vote-game/viewer" target="_blank"  style="color: white;">
                                                                    Vote Viewer Page
                                                                </a>
                                                            </button>
                                                        </div>
                                                    </div>
                                                    
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="card card-success">
                                                        <div class="card-header">
                                                            <h3 class="card-title font-weight-bold">
                                                              Danh sách tiêu chí chấm điểm
                                                            </h3>
                                                            <div class="card-tools">
                                                                <button type="button" class="btn btn-success btn-sm"
                                                                    data-card-widget="collapse" title="Collapse">
                                                                    <i class="fas fa-minus"></i>
                                                                </button>
                                                            </div>
                                                        </div>
                                                        <div class="card-body card-overflow" style="max-height: 42vh;">
                                                            <table class="table table-bordered">
                                                                <thead>
                                                                    <tr>
                                                                        <th style="width: 10px">STT</th>
                                                                        <th>Tiêu chí</th>
                                                                        <th>Điểm</th>
                                                                    </tr>
                                                                </thead>
                                                                <tbody>
                                                                    <% gameData.point_ladder.forEach((point_ladder,index)=> { %>
                                                                        <tr>
                                                                            <td><%-index+1%></td>
                                                                            <td><%-point_ladder.title%></td>
                                                                            <td><%-point_ladder.max_point%></td>
                                                                        </tr>
                                                                    <% }) %>
                                                                </tbody>
                                                            </table>

                                                        </div>
                                                    </div>
                                                    
                                                </div>
                                            </div>
                                        </div>
                                        <!-- /.card-body -->

                                        <div class="card-footer" style="display: flex ;justify-content: space-between;">
                                           <button id="update_game_infor_btn" class="btn btn-success col-sm-4 ">Cập Nhật Thông Tin</button>
                                             
                                            <% if (gameData.game.active == 0) { %>
                                                <button class="btn btn-success col-sm-4" id="game_start_btn">Bắt Đầu Cuộc Thi</button>
                                            <% } %>
                                            
                                            <% if (gameData.game.active == 1) { %>
                                                <button id="export_report"  class="btn btn-success col-sm-3 ">Xuất Báo Cáo</button>
                                            <% } %>

                                            <% if (gameData.game.active == 0) { %>
                                                <% if (gameData.candidate.data[0] == null && gameData?.candidateList[0] == null && gameData.examiner.data[0] == null) { %>
                                                    <button class="btn btn-danger col-sm-3 " id="game_stop_btn">Huỷ Cuộc Thi</button>
                                                <% } %>
                                            <% } %>                                                                            
                                        </div>
                                    </div>
                                    <!-- /.card -->
                                </div>

                                <div class="col-12 col-sm-6 col-md-3">
                                    <div class="card card-success">
                                        <div class="card-header">
                                            <h3 class="card-title font-weight-bold">Tiết Mục Đang Dự Thi</h3>
                                        </div>
                                        <!-- /.card-header -->
                                        <% if (gameData.curent_candidate.fullname == null) { %>
                                            <div class="card-body" style="height: 500px;">
                                            </div>
                                        <% } else { %>
                                            <div class="card-body" style="height: 500px;">
                                                <div class="info-box mb-3">
                                                    <span class="info-box-icon bg-warning elevation-1"><img
                                                        src="/template/image/<%-gameData.curent_candidate.avatar%>"
                                                            alt=""
                                                            srcset=""></span>

                                                    <div class="info-box-content">
                                                        <span class="info-box-text">
                                                            <%- gameData.curent_candidate.fullname %>
                                                        </span>
                                                        <span class="info-box-text">
                                                            <%- gameData.curent_candidate.title %>
                                                        </span>
                                                    </div>
                                                    <!-- /.info-box-content -->
                                                </div>

                                                <div class="info-box">
                                                    <span class="info-box-icon bg-info elevation-1"><i
                                                            class="fas fa-chalkboard-teacher"></i></span>

                                                    <div class="info-box-content">
                                                        <span class="info-box-text">Tổng số điểm</span>
                                                        <span id="candidate_total_point_dashboard" class="info-box-number">
                                                            <%- gameData.curent_candidate.point.toFixed(1) ?? 0%>
                                                        </span>
                                                    </div>
                                                    <!-- /.info-box-content -->
                                                </div>

                                                <div class="info-box mb-3">
                                                    <span class="info-box-icon bg-primary elevation-1"><i
                                                            class="fas fa-thumbs-up"></i></span>

                                                    <div class="info-box-content">
                                                        <span class="info-box-text">Tổng số likes</span>
                                                        <span class="info-box-number" id="current_number_vote">
                                                            <%- gameData.curent_candidate.ratting %>

                                                        </span>
                                                    </div>
                                                    <!-- /.info-box-content -->
                                                </div>

                                            </div>
                                        <% } %>

                                        <div class="card-footer">
                                            <!-- <button class="btn btn-primary col-sm-3">Vote</button>
                                            <button class="btn btn-success ">Chấm Điểm</button> -->

                                        <% if (gameData.curent_candidate.id_candidate == null) { %>
                                            <button class="btn btn-danger col-12 finish-candidate" disabled>Kết Thúc Phần Thi</button>
                                         
                                        <% } else {%>
                                            <button class="btn btn-danger col-12 finish-candidate" data-user="<%- gameData.curent_candidate.id_candidate %>">Kết Thúc Phần Thi</button>
                                        <% } %>
                                            
                                        </div>
                                    </div>
                                </div>
                                <div class="col-12 col-sm-6 col-md-3">
                                    <div class="card card-success">
                                        <div class="card-header">
                                            <h3 class="card-title font-weight-bold">Bảng Điểm Giám Khảo</h3>
                                        </div>
                                        <!-- /.card-header -->
                                        <div class="card-body" style="height: 500px;">
                                            <div class="examiner_body_card_list">

                                                <% gameData.point_current_candidate.forEach((point,index)=> { %>   
                                                    <div class="info-box mb-3">
                                                        <span class="info-box-icon bg-warning elevation-1"><img
                                                            src="/template/image/<%-point.examiner.avatar %>" alt=""
                                                                srcset=""></span>

                                                        <div class="info-box-content">
                                                            <span class="info-box-text"><%-point.examiner.fullname %></span>
                                                            <span class="info-box-text"><%-point.examiner.title %></span>
                                                        </div>
                                                        <span class="info-box-icon bg-primary elevation-1">
                                                            <p><%-point.point %></p>
                                                        </span>

                                                        <!-- /.info-box-content -->
                                                    </div>
                                                <% }) %>

                                            </div>



                                        </div>
                                        <div class="card-footer">
                                            <button id="examiner_total_point_dashboard" class="btn btn-success col-12" disabled>Tổng Điểm: <%- gameData.curent_candidate?.point?.toFixed(1) ?? 0 %></button>
                                        </div>
                                    </div>
                                </div>

                            </div>
                          
                            

                            <div class="row mb-2">
                                <div class="col-md-12">
                                    <div class="card">
                                        <div class="card-header bg-warning flex">
                                            <h3 class="card-title font-weight-bold">Danh Sách Thí Sinh</h3>
                                            <div class="card-tools">

                                                <button type="button" class="btn btn-warning btn-sm"
                                                    data-card-widget="collapse" title="Collapse">
                                                    <i class="fas fa-minus"></i>
                                                </button>
                                            </div>
                                        </div>
                                        <!-- /.card-header -->
                                        <div class="card-body card-overflow">
                                            <table class="table table-bordered">
                                                <thead>
                                                    <tr>
                                                        <th style="width: 10px">STT</th>
                                                        <th style="width: 80px; text-align: center;">Avatar</th>
                                                        <th>Tên thí sinh</th>
                                                        <th>Tên phần thi</th>
                                                        <th style="width: 100px;">Trạng thái</th>
                                                        <th style="width: 20px">Điểm</th>
                                                        <th style="width: 20px">Like</th>
                                                        <th style="width: 170px">Cập nhật</th>
                                                        <th>Bảng điều khiển</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                  <% gameData.candidate.data.forEach((candidate,index)=> { %>                                                 
                                                    <% if (candidate.type == 'SINGLE') { %>
                                                        <tr>
                                                            <td><%-index+1%></td>
                                                            <td><img class="table_avatar"
                                                                    src="/template/image/<%-candidate.avatar %>" alt="" srcset="">
                                                            </td>
                                                            <td>
                                                                <p><%-candidate.fullname %></p>
                                                            </td>
                                                            <td>
                                                                <p><%-candidate.title %></p>
                                                            </td>
    
                                                            
    
                                                            <td>
                                                                <% if (candidate.active == 0) { %>
                                                                    <span class="badge bg-primary">
                                                                        CHƯA THI
                                                                    </span>
                                                                <% } %>
                                                                <% if (candidate.active == 1) { %>
                                                                    <span class="badge bg-success">
                                                                         ĐANG THI
                                                                    </span>
                                                                <% } %>
                                                                <% if (candidate.active == 2) { %>
                                                                    <span class="badge bg-warning">
                                                                        ĐÃ THI
                                                                    </span>
                                                                <% } %>
                                                            </td>
                                                            <td><span class="badge bg-primary"><%-candidate.dataValues.total_point || 0%></span></td>
                                                            <td><span class="badge bg-primary"><%-candidate.ratting%></span></td>
                                                            <td>
                                                                <button type="submit" data-user="<%-candidate.id_candidate%>" class="btn btn-warning update-candidate">Cập nhật</button>
                                                                <button type="submit" data-user="<%-candidate.id_candidate%>" class="btn btn-danger delete-candidate">Xoá</button>
    
                                                            </td>
                                                            <td>
                                                                <% if (candidate.active == 0) { %>
                                                                    <button type="submit" data-user="<%-candidate.id_candidate%>" class="btn btn-primary start-candidate">Bắt đầu thi</button>
                                                                <% } %>
                                                            </td>
                                                        </tr>
                                                    <% } %>
                                                  <% }) %>
                                                </tbody>
                                            </table>
                                        </div>
                                        <!-- /.card-body -->
                                        <div class="card-footer clearfix">
                                            <button type="submit" class="btn btn-warning col-sm-3 float-right" id="add_new_candidate_btn">Thêm Thí
                                                Sinh</button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row mb-2">
                                <div class="col-md-12">
                                    <div class="card">
                                        <div class="card-header bg-primary">
                                            <h3 class="card-title font-weight-bold">Danh Sách Ban Giám Khảo</h3>
                                            <div class="card-tools">

                                                <button type="button" class="btn btn-primary btn-sm"
                                                    data-card-widget="collapse" title="Collapse">
                                                    <i class="fas fa-minus"></i>
                                                </button>
                                            </div>
                                        </div>
                                        <!-- /.card-header -->
                                        <div class="card-body card-overflow">
                                            <table class="table table-bordered">
                                                <thead>
                                                    <tr>
                                                        <th style="width: 10px">ID</th>
                                                        <th style="width: 80px; text-align: center;">Avatar</th>
                                                        <th>Username</th>
                                                        <th>Giám khảo</th>
                                                        <th>Chức danh</th>
                                                        <th style="width: 100px; text-align: center;">Ẩn điểm</th>
                                                        <th>Cập nhật</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                  <% gameData.examiner.data.forEach((examiner,index)=> { %>                                                 
                                                    <tr>
                                                        <td><%- index+1 %></td>
                                                        <td><img class="table_avatar"
                                                            src="/template/image/<%-examiner.avatar %>" alt="" srcset="">
                                                        </td>
                                                        <td>
                                                            <p><%-examiner.username %></p>
                                                        </td>
                                                        <td>
                                                            <p><%-examiner.fullname %></p>
                                                        </td>
                                                        <td>
                                                            <p><%-examiner.title %></p>
                                                        </td>
                                                        <td style="text-align: center;">
                                                            <input 
                                                                <% if (examiner.hidden == 1) { %>
                                                                    checked
                                                                <% } %>
                                                                type="checkbox" 
                                                                class="hidden-examiner" 
                                                                data-user="<%-examiner.id_examiner%>">
                                                        </td>
                                                        <td>
                                                            <button type="submit" data-user="<%-examiner.id_examiner%>" class="btn btn-warning update-examiner">Cập nhật</button>
                                                            <button type="submit" data-user="<%-examiner.id_examiner%>" class="btn btn-danger delete-examiner">Xoá</button>

                                                        </td>
                                                    </tr>
                                                  <% }) %>
                                                </tbody>
                                            </table>
                                        </div>
                                        <!-- /.card-body -->
                                        <div class="card-footer clearfix">
                                            <button type="submit" id="add_new_examiner_btn" class="btn btn-primary col-sm-3 float-right">Thêm Giám
                                                Khảo</button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <% if (gameData?.candidateList[0]?.active != null) { %>
                                <div class="row mb-2">
                                    <div class="col-md-12">
                                        <div class="card">
                                            <div class="card-header bg-success flex">
                                                <h3 class="card-title font-weight-bold">Danh Sách Bình Chọn</h3>
                                                <div class="card-tools">
    
                                                    <button type="button" class="btn btn-success btn-sm"
                                                        data-card-widget="collapse" title="Collapse">
                                                        <i class="fas fa-minus"></i>
                                                    </button>
                                                </div>
                                            </div>
                                            <!-- /.card-header -->
                                            <div class="card-body card-overflow">
                                                <table class="table table-bordered">
                                                    <thead>
                                                        <tr>
                                                            <th style="width: 10px">STT</th>
                                                            <th style="width: 80px; text-align: center;">Avatar</th>
                                                            <th>Tên thí sinh</th>
                                                            <th>Tên phần thi</th>
                                                            <th style="width: 20px">Vote</th>
                                                            <th style="width: 170px">Cập nhật</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                      <% gameData.candidateList.forEach((candidate,index)=> { %>                                                 
                                                            <tr>
                                                                <td><%-index+1%></td>
                                                                <td><img class="table_avatar"
                                                                        src="/template/image/<%-candidate.avatar %>" alt="" srcset="">
                                                                </td>
                                                                <td>
                                                                    <p><%-candidate.fullname %></p>
                                                                </td>
                                                                <td>
                                                                    <p><%-candidate.title %></p>
                                                                </td>
        
                                                               
                                                                <td><span class="badge bg-primary"><%-candidate.ratting%></span></td>
                                                                <td>
                                                                    
                                                                    <button type="submit" data-user="<%-candidate.id_candidate%>" class="btn btn-warning update-candidate">Cập nhật</button>
                                                                    <button type="submit" data-user="<%-candidate.id_candidate%>" class="btn btn-danger delete-candidate">Xoá</button>
                                                                </td>
                                                                
                                                            </tr>
                                                      <% }) %>
                                                    </tbody>
                                                </table>
                                            </div>
                                            <!-- /.card-body -->
                                            <div class="card-footer clearfix">
                                                <% if (gameData?.candidateList[0]?.active == 0) { %>
                                                    <button type="submit" class="btn btn-success col-sm-3 float-right" id="start_vote_list">Bắt Đầu Bình Chọn</button>
    
                                                <% } %>
                                                <% if (gameData?.candidateList[0]?.active == 1) { %>
                                                    <button type="submit" class="btn btn-danger col-sm-3 float-right" id="stop_vote_list">Kết Thúc Bình Chọn</button>
    
                                                <% } %>
                                                <% if (gameData?.candidateList[0]?.active == 2) { %>
                                                    <button type="submit" disabled class="btn btn-danger col-sm-3 float-right">Đã Kết Thúc Bình Chọn</button>
    
                                                <% } %>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            <% } %>
                            
                            <% if (gameData.game.active == 1 ) { %>
                                <div class="card-footer clearfix">
                                    <button class="btn btn-danger col-sm-4 float-right" id="game_stop_btn">Kết Thúc Cuộc Thi</button>
                                </div>
                            <% } %>

                          </div>
                          <!-- End Game Box -->

                        <% } else {%>
                          <!-- New Game Box -->
                          <form id="form_create_game">
                            <div class="card card-primary row ">
                                <div class="card-header">
                                    <h3 class="card-title">Tạo Cuộc Thi Mới</h3>
                                </div>
                                <div class="card-body row">
                                    <div class="col-md-6">
                                        <!-- general form elements -->
                                        <div class="card card-success">
                                            <div class="card-header">
                                                <h3 class="card-title">Thông Tin Cuộc Thi</h3>
                                            </div>
                                            <!-- /.card-header -->
                                            <!-- form start -->
                                                <div class="card-body">
                                                    <div class="form-group">
                                                        <label for="name_game">Tên cuộc thi </label>
                                                        <input type="text" required class="form-control" id="name_game"
                                                            placeholder="Nhập tên cuộc thi">
                                                    </div>
                                                    <div class="form-group">
                                                        <label id="upload_label" for="input_background">
                                                            <img src="/template/image/img_upload_icon.png" alt="">
                                                            Chọn ảnh Background
                                                        </label>
                                                        <input accept="image/*" id="input_background" name="input_background" type="file" hidden>
                                                    </div>
                                                    <div class="form-group">
                                                        <label for="max_vote">Số lượt bình chọn: </label>
                                                        <input type="number" required class="form-control" id="max_vote"
                                                            placeholder="Tổng số thí sinh được bình chọn trong một lượt vote"  min="1" >
                                                    </div>
                                                </div>
                                                <!-- /.card-body -->
        
                                        </div>
                                        <!-- /.card -->
                                    </div>
        
                                    <div class="col-md-6">
                                        <!-- general form elements -->
                                        <div class="card card-warning">
                                            <div class="card-header">
                                                <h3 class="card-title">Tiêu Chí Chấm Điểm</h3>
                                            </div>
                                            <!-- /.card-header -->
                                            <!-- form start -->
                                                <div class="card-body" id="form_point_ladder">
                                                    <label for="name_game" >Tiêu chí chấm điểm</label>
                                                    <div class="form-group row" style="margin-left: 0;">
                                                        <input type="text" required class="col form-control point_ladder_title"
                                                        placeholder="Nhập tiêu chí" >
                                                        <input type="number" step="0.5" min="0" required class="col-3 form-control point_ladder_max_point"
                                                        placeholder="Nhập điểm"
                                                        style="margin-left: 10px;"
                                                        >
                                                        <button type="button" class="btn"  style="margin-left: 10px; opacity: 0; cursor: inherit;">X</button>
                                                    </div>
                                                    
                                                </div>
                                                <div class="card-footer">
                                                    <button type="button" id="add_more_point_ladder_button" class="btn btn-warning col-6">Thêm tiêu chí</button>
                                                </div>
                                                <!-- /.card-body -->
                                        </div>
                                        <!-- /.card -->
                                    </div>
                                </div>
                                <div class="card-footer">
                                    <button type="submit" id="submit_form_create_game" class="btn btn-success col-sm-3 float-right">Tạo Cuộc Thi</button>
                                </div>
                            </div>
                          </form>
                           
                          <!-- End New Game Box -->  
                        <% } %>
                    </div>

                </section>
                <!-- /.content -->
            </div>
            <!-- /.content-wrapper -->
            <footer class="main-footer">
                <strong>Copyright &copy; 2023 <a href="https://truegame.vn">truegame.vn</a>.</strong>
                All rights reserved.
                <div class="float-right d-none d-sm-inline-block">
                    <b>Version</b> 1.1.0
                </div>  
            </footer>

            <!-- Control Sidebar -->
            <aside class="control-sidebar control-sidebar-dark">
                <!-- Control sidebar content goes here -->
            </aside>
            <!-- /.control-sidebar -->
        </div>
        <!-- ./wrapper -->

        

        <%- include('script'); -%>
    </body>

</html>
